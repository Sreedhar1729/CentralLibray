<mvc:View
    xmlns:form="sap.ui.layout.form"
    controllerName="com.app.centrallibrary.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:layout="sap.ui.layout"
    xmlns:f="sap.f"
>
    <f:DynamicPage
        id="idBooksListPageTitle"
        headerExpanded="true"
    >
        <f:title>
            <f:DynamicPageTitle id="idBooksDynamicPageTitle">
                <f:heading>
                    <Title
                        id="idBooksTitle"
                        text="{i18n>title}"
                    />
                </f:heading>
                <f:expandedContent>
                    <Label
                        id="idBooksListPageECL"
                        text="{i18n>flpSubtitle}"
                    />
                </f:expandedContent>
                <f:snappedContent>
                    <Label
                        id="idBooksListPageESL"
                        text="{i18n>flpSubtitle}"
                    />
                </f:snappedContent>
                <f:snappedTitleOnMobile>
                    <Title
                        id="idBooksListPageEST"
                        text="{i18n>flpflpSubtitle}"
                    />
                </f:snappedTitleOnMobile>
                <f:navigationActions>
                  <Button
                id="_IDGenButton1"
                text="Admin"
                press="onAdminButton"
                
                
            />
            <Button
                id="_IDGenButton2"
                text="User"
                press="onUserLogin"
                
                
            />
                     
                </f:navigationActions>
            </f:DynamicPageTitle>
        </f:title>
    
    <f:content>

   
</f:content>
</f:DynamicPage>
