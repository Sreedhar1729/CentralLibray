<mvc:View controllerName="com.app.centrallibrary.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m" 
    xmlns:layout="sap.ui.layout"
    xmlns:f="sap.f">

    <f:DynamicPage id="_IDGenDynamicPage1" headerExpanded="true">
    <f:title>
    <f:DynamicPageTitle id="_IDGenDynamicPageTitle1" >
    <f:heading>
    <Title id="_IDGenTitle1" text="CentralLibraySystemAPP"></Title>
    </f:heading>
    </f:DynamicPageTitle>
    </f:title>

    <!-- <f:header>
     <f:DynamicPageHeader
                id="idDynamicPageHeader"
                pinnable="true"
            >
            <HBox id="_IDGenHBox1" alignContent="SpaceAround"></HBox>
            <VBox id="_IDGenVBox1"  class="sapUiSmallMarginEnd"></VBox>
            <Label id="_IDGenLabel1"  text="Book ISBN"></Label>
    </f:DynamicPageHeader>
    </f:header> -->
    <f:content>
    <Table id="_IDGenTable1" mode="SingleSelectMaster" alternateRowColors="true" items="{/Books}" >
    <headerToolbar>
    <OverflowToolbar id="_IDGenOverflowToolbar1" >
    <Title id="_IDGenTitle2" titleStyle="H6" text="Books">
    </Title></OverflowToolbar>
    </headerToolbar>

    <columns>
    <Column id="_IDGenColumn1" >
    <Title id="_IDGenTitle3" text="ISBN" ></Title>
    </Column>

     <Column id="_IDGenColumn2" >
    <Title id="_IDGenTitle4" text="Title" ></Title>
    </Column>

     <Column id="_IDGenColumn3" >
    <Title id="_IDGenTitle5" text="Author" ></Title>
    </Column>

     <Column id="_IDGenColumn4" >
    <Title id="_IDGenTitle6" text="Price" ></Title>
    </Column>
     <Column id="_IDGenColumn5" >
    <Title id="_IDGenTitle7" text="Status" ></Title>
    </Column>
    </columns>

    <items>
     <ColumnListItem id="_IDGenColumnListItem1"  type="Navigation">
     <cells>
     <Text id="_IDGenText1"  text="{isbn}"></Text>
      <Text id="_IDGenText2"  text="{title}"></Text>
       <Text id="_IDGenText3"  text="{author}"></Text>
        <Text id="_IDGenText4"  text="{price}"></Text>
         <Text id="_IDGenText5"  text="{status}"></Text>
     </cells>
     
     </ColumnListItem>

    </items>
    
    
    </Table>
    </f:content>
    
    </f:DynamicPage>
      
</mvc:View>
